{
    "manifest_version":2,
    "name":"TemplateJsonApi",
    "version":"0.0.1",
    "author":[
        "[https://github.com/simontaurus Simon Stier]"
    ],
    "url":"https://github.com/simontaurus/mediawiki-extensions-TemplateJsonApi",
    "descriptionmsg":"template-json-api-desc",
    "namemsg":"template-json-api-name",
    "license-name":"AGPL-3.0-or-later",
    "type":"editor",
    "requires":{
        "MediaWiki":">= 1.31"
    },
    "MessagesDirs":{
        "TemplateJsonApi":[
            "i18n"
        ]
    },
    "ResourceModules":{
        "ext.TemplateJsonApi":{
            "localBasePath":"modules/ext.TemplateJsonApi",
            "remoteExtPath":"TemplateJsonApi/modules/ext.TemplateJsonApi",
            "styles":[
                "TemplateJsonApi.css"
            ],
            "scripts":[
                "TemplateJsonApi.js"
            ],
            "position":"bottom"
        }
    },
    "AutoloadClasses":{
        "TemplateJsonApi":"includes/TemplateJsonApi.php"
    },
    "Hooks":{
        "BeforePageDisplay":"TemplateJsonApi::onBeforePageDisplay"
    }
}
